# Core Configuration
NODE_ENV=development
PORT=3000

# Gateway Connection
GATEWAY_URL=http://localhost:3000
GATEWAY_TOKEN=your-gateway-token-here

# Dashboard Settings
DASHBOARD_TITLE="OpenClaw Dashboard"
DASHBOARD_LOGO_URL="/logo.png"

# Feature Flags
ENABLE_REALTIME=true
ENABLE_CHARTS=true
ENABLE_ALERTS=false

# Logging
LOG_LEVEL=info
LOG_FORMAT=combined

# Security
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=24h
